<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - Known</title>
    @Html.MinStyle("static/modules/frame.min.css")
    @RenderSection("Head", false)
</head>
<body>
    @Html.AntiForgeryToken()
    @RenderBody()
    <div id="dialog" class="mini-window" title="窗体" iconCls="fa-windows"
         showModal="true" showShadow="true" 
         showMaxButton="true" showCollapseButton="true" 
         allowResize="true" allowDrag="true">
    </div>
    @Html.MinScript("static/modules/frame.min.js")
    @RenderSection("Foot", false)
</body>
</html>